{
  "name": "express-demo",
  "version": "1.0.0",
  "description": "Express Demo",
  "scripts": {
    "prestart": "npm install",
    "start": "pm2 start server.js --name=express-demo-14874",
    "poststart": "cp nginx.conf /etc/nginx/conf.d/demo14874.errsole.com.conf && sudo service nginx reload",
    "prestop": "rm -f /etc/nginx/conf.d/demo14874.errsole.com.conf && sudo service nginx reload",
    "stop": "pm2 delete express-demo-14874"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "errsole": "^1.3.4",
    "express": "^4.17.1",
    "multer": "^1.4.3"
  }
}
